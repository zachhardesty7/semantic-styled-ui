{
  "name": "semantic-styled-ui",
  "version": "0.32.1",
  "description": "Set of UI components based of Semantic UI with Styled Components",
  "keywords": [
    "semantic",
    "semantic-ui",
    "styled",
    "components",
    "styled-components",
    "ui",
    "react",
    "gatsby"
  ],
  "homepage": "https://github.com/zachhardesty7/semantic-styled-ui",
  "bugs": "https://github.com/zachhardesty7/semantic-styled-ui/issues",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/types.d.ts",
  "license": "MIT",
  "author": "Zach Hardesty <hello@zachhardesty.com> (https://zachhardesty.com)",
  "repository": "github:zachhardesty7/semantic-styled-ui",
  "browserslist": [
    ">0.25%",
    "not dead"
  ],
  "eslintConfig": {
    "extends": "hardesty/prettier"
  },
  "prettier": {
    "semi": false
  },
  "scripts": {
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "clean": "rimraf dist/*",
    "clean:full": "rimraf dist/* node_modules yarn.lock && yarn",
    "build": "yarn clean && rollup --config --minifyInternalExports --compact --environment ENV_MODE:prod",
    "build:dev": "rollup --config --environment ENV_MODE:local",
    "watch": "rollup --config --watch --sourcemap --environment ENV_MODE:local",
    "dev": "yarn watch",
    "pub": "publish",
    "release": "yarn build && yarn pub"
  },
  "dependencies": {
    "@types/react": "^16.9.44",
    "prop-types": "^15.7.2",
    "react-scroll": "^1.8.0",
    "semantic-ui-react": "^1.1.0",
    "set-wrapper-display-name": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "7.10.5",
    "@babel/core": "7.11.0",
    "@babel/plugin-transform-react-jsx-source": "^7.10.5",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@quickbaseoss/babel-plugin-styled-components-css-namespace": "^1.0.1",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "5.1.0",
    "@rollup/plugin-commonjs": "14.0.0",
    "@rollup/plugin-node-resolve": "8.4.0",
    "@types/styled-components": "5.1.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "eslint": "7.6.0",
    "eslint-config-hardesty": "^3.3.1",
    "glob": "^7.1.6",
    "prettier": "^2.0.5",
    "publish-me-maybe": "^1.0.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "react-styleguidist": "^11.0.8",
    "rimraf": "^3.0.2",
    "rollup": "2.23.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-terser": "^6.1.0",
    "semantic-ui-css": "^2.4.1",
    "styled-components": "^5.1.1",
    "stylelint": "^13.6.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript-to-proptypes": "^2.1.0"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "semantic-ui-css": "^2.4.1",
    "styled-components": "^5.1.1"
  }
}
