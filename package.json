{
  "name": "semantic-styled-ui",
  "version": "0.18.1",
  "description": "Set of UI components based of Semantic UI with Styled Components",
  "keywords": [
    "semantic",
    "semantic-ui",
    "styled",
    "components",
    "styled-components",
    "ui",
    "react",
    "gatsby"
  ],
  "homepage": "https://github.com/zachhardesty7/semantic-styled-ui",
  "bugs": "https://github.com/zachhardesty7/semantic-styled-ui/issues",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "index.d.ts",
  "license": "MIT",
  "author": "Zach Hardesty <hello@zachhardesty.com> (https://zachhardesty.com)",
  "repository": "github:zachhardesty7/semantic-styled-ui",
  "browserslist": [
    ">0.25%",
    "not dead"
  ],
  "eslintConfig": {
    "extends": "hardesty"
  },
  "scripts": {
    "ls": "sh scripts/ls.sh",
    "lsa": "sh scripts/lsa.sh",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "clean": "rimraf dist/*",
    "clean:full": "rimraf dist/* node_modules yarn.lock && yarn",
    "babel": "ENV_MODE=local babel ./src --out-dir ./dist --copy-files",
    "build": "yarn clean && rollup --config --minifyInternalExports --compact --environment ENV_MODE:prod",
    "build:dev": "yarn clean && rollup --config --sourcemap --environment ENV_MODE:local",
    "watch": "rollup --config --watch --sourcemap --environment ENV_MODE:local",
    "dev": "yarn watch",
    "pub:yarn": "yarn build && yarn publish",
    "pub": "yarn build && npm publish"
  },
  "dependencies": {
    "@types/react": "^16.9.38",
    "prop-types": "^15.7.2",
    "react-scroll": "^1.7.16",
    "semantic-ui-react": "^0.88.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@quickbaseoss/babel-plugin-styled-components-css-namespace": "^1.0.1",
    "@rollup/plugin-babel": "^5.0.3",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "eslint": "^7.3.0",
    "eslint-config-hardesty": "^1.9.3",
    "glob": "^7.1.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "react-styleguidist": "^11.0.8",
    "rimraf": "^3.0.2",
    "rollup": "^2.18.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-terser": "^6.1.0",
    "semantic-ui-css": "^2.4.1",
    "styled-components": "^5.1.1",
    "stylelint": "^13.6.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript-to-proptypes": "^2.1.0"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "semantic-ui-css": "^2.4.1",
    "styled-components": "^5.1.1"
  }
}
