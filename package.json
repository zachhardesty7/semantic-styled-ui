{
  "name": "semantic-styled-ui",
  "version": "0.10.0",
  "description": "Set of UI components based of Semantic UI with Styled Components",
  "keywords": [
    "semantic",
    "semantic-ui",
    "styled",
    "components",
    "styled-components",
    "ui",
    "react",
    "gatsby"
  ],
  "homepage": "https://github.com/zachhardesty7/semantic-styled-ui",
  "bugs": "https://github.com/zachhardesty7/semantic-styled-ui/issues",
  "main": "dist/index.js",
  "module": "dist/my-library.es.js",
  "license": "MIT",
  "author": "Zach Hardesty <hello@zachhardesty.com> (https://zachhardesty.com)",
  "repository": "github:zachhardesty7/semantic-styled-ui",
  "browserslist": [
    ">0.25%",
    "not dead"
  ],
  "eslintConfig": {
    "extends": "hardesty"
  },
  "scripts": {
    "link:ls": "sh scripts/ls.sh",
    "link:lsa": "sh scripts/lsa.sh",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "clean": "rimraf dist/*",
    "babel": "babel ./src --out-dir ./dist --copy-files",
    "build": "rollup -c",
    "watch": "rollup --config --watch --sourcemap",
    "dev": "yarn clean && yarn watch",
    "pub:yarn": "yarn build && yarn publish",
    "pub": "yarn build && npm publish"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "semantic-ui-react": "^0.88.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/plugin-transform-template-literals": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@quickbaseoss/babel-plugin-styled-components-css-namespace": "^1.0.0-rc4",
    "@rollup/plugin-babel": "^5.0.2",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "cross-env": "^7.0.2",
    "eslint": "^7.0.0",
    "eslint-config-hardesty": "^1.9.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-styleguidist": "^11.0.6",
    "rimraf": "^3.0.2",
    "rollup": "^2.10.5",
    "semantic-ui-css": "^2.4.1",
    "styled-components": "^5.1.0",
    "stylelint": "^13.4.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "webpack": "^4.43.0"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "semantic-ui-css": "^2.4.1",
    "styled-components": "^5.1.0"
  }
}
